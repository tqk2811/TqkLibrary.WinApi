<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<!--https://docs.microsoft.com/en-us/nuget/reference/nuspec-->
	<metadata>
		<id>$id$</id>
		<version>1.0.0-build$buildDay$</version>
		<description>my private lib</description>
		<authors>tqk2811</authors>
		<readme>docs\README.MD</readme>
		<projectUrl>https://github.com/tqk2811/TqkLibrary.WinApi</projectUrl>
		<repository type="git" url="https://github.com/tqk2811/TqkLibrary.WinApi.git"/>
		<license type="expression">MIT</license>
		<dependencies>
			<group targetFramework="net462">
				<dependency id="Microsoft.Win32.Registry" version="5.0.0" />
				<dependency id="System.Management" version="8.0.0" />
				<dependency id="System.Drawing.Common" version="8.0.5" />
				<dependency id="System.Memory" version="4.5.5" />
				<dependency id="System.Runtime.CompilerServices.Unsafe" version="6.0.0" />
				<dependency id="System.ValueTuple" version="4.5.0" />
			</group>
			<group targetFramework="net6.0-windows10.0.17763.0">
				<dependency id="Microsoft.Win32.Registry" version="5.0.0" />
				<dependency id="System.Management" version="8.0.0" />
				<dependency id="System.Drawing.Common" version="8.0.5" />
				<dependency id="System.Memory" version="4.5.5" />
				<dependency id="System.Runtime.CompilerServices.Unsafe" version="6.0.0" />
				<dependency id="System.ValueTuple" version="4.5.0" />
			</group>
			<group targetFramework="net8.0-windows10.0.17763.0">
				<dependency id="Microsoft.Win32.Registry" version="5.0.0" />
				<dependency id="System.Management" version="8.0.0" />
				<dependency id="System.Drawing.Common" version="8.0.5" />
				<dependency id="System.Memory" version="4.5.5" />
				<dependency id="System.Runtime.CompilerServices.Unsafe" version="6.0.0" />
				<dependency id="System.ValueTuple" version="4.5.0" />
			</group>
		</dependencies>
	</metadata>
	<files>
		<file src="bin\Release\**\$id$.dll" target="lib\" />
		<file src="bin\Release\**\$id$.pdb" target="lib\" />
		<file src="bin\Release\**\$id$.xml" target="lib\" />
		<file src="**\*.cs" target="src\" exclude="obj\**\*.cs;bin\**\*.cs"/>
		<file src="..\README.MD" target="docs\" />
	</files>
</package>